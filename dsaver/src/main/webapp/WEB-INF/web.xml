<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DataSaver</servlet-name>
  	<display-name>DataSaver</display-name>
  	<description>Saving major repository data into integrated database</description>
  	<servlet-class>com.eptd.dsaver.servlet.DataSaver</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TaskDistributor</servlet-name>
  	<display-name>TaskDistributor</display-name>
  	<description></description>
  	<servlet-class>com.eptd.dsaver.servlet.TaskDistributor</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TaskPoster</servlet-name>
  	<display-name>TaskPoster</display-name>
  	<description></description>
  	<servlet-class>com.eptd.dsaver.servlet.TaskPoster</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ClientInfo</servlet-name>
  	<display-name>ClientInfo</display-name>
  	<description></description>
  	<servlet-class>com.eptd.dsaver.servlet.ClientInfo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HeaderTester</servlet-name>
  	<display-name>HeaderTester</display-name>
  	<description></description>
  	<servlet-class>com.eptd.dsaver.servlet.HeaderTester</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DataSaver</servlet-name>
  	<url-pattern>/DataSaver</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TaskDistributor</servlet-name>
  	<url-pattern>/TaskDistributor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TaskPoster</servlet-name>
  	<url-pattern>/TaskPoster</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClientInfo</servlet-name>
  	<url-pattern>/ClientInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HeaderTester</servlet-name>
  	<url-pattern>/HeaderTester</url-pattern>
  </servlet-mapping>
</web-app>
